{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/server.js"
      }
    ],
    "env": {
      "NEXT_PUBLIC_FRONTEND_URL": "https://www.owngoalproject.com"
    }
  }